        <ul>
            {# vytvorila umela intelginece, a je to takova docasna znouzectnost #}
            {% if category is iterable and category|default([]) is same as(category) %}
                {% set category = category|default([])|sort((a, b) => a.rankInMenu > b.rankInMenu ? 1 : -1) %}
            {% endif %}
            {# konec UI #}

          {% for item in category %}
          	<li>
      	      	<a href="{{ path('category', {'seoTitle': item.seoTitle}) }}">{{ item.name }}</a>
    	        {% if item.categories is not empty %}
	                {% include 'frontend/category/category.html.twig' with {'category':item.categories} %}
       	        {% endif %}
             </li>
          {% endfor %}
        </ul>
